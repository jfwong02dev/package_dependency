{
  "name": "package-dependency",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.16.6",
    "axios": "^0.18.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-import": "^1.11.0"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "dynamic-import-node"
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "lib"
        }
      ],
      [
        "react-hot-loader/babel"
      ]
    ]
  }
}
